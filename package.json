{
  "name": "chrome-extension-bsky-fav-mc",
  "version": "1.0.4",
  "description": "Chrome extension for Bluesky saved posts middle-click navigation",
  "main": "background.js",
  "scripts": {
    "build": "npm run cleanup && node scripts/build.js",
    "cleanup": "rimraf --glob dist *.dist.zip",
    "zip": "npm run build && npm-build-zip --source=./dist --name=dist"
  },
  "keywords": ["chrome extension", "bluesky", "convenience"],
  "author": "Marco Beier",
  "license": "Unlicense",
  "type": "module",
  "devDependencies": {
    "@types/chrome": "^0.1.28",
    "npm-build-zip": "^1.0.3",
    "rimraf": "^5.0.5",
    "terser": "^5.36.0"
  }
}
